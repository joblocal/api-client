{
  "name": "@joblocal/api-client",
  "version": "1.3.0",
  "description": "This repository provides an api-client to use the joblocal api.",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && npm run validate",
    "build": "webpack -p",
    "test": "jest src",
    "test:watch": "npm test -- --watch --onlyChanged",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext js --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "validate": "npm-run-all --parallel lint test"
  },
  "author": "Joblocal GmbH",
  "license": "ISC",
  "dependencies": {
    "devour-client": "^2.0.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "git-hooks": "^1.1.10",
    "jest": "^24.7.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
