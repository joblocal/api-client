{
  "name": "@joblocal/api-client",
  "version": "2.6.2",
  "description": "This repository provides an api-client to use the joblocal api.",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && npm run validate",
    "build": "webpack -p",
    "test": "jest src",
    "test:watch": "npm test -- --watch --onlyChanged",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext js --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "validate": "npm-run-all --parallel lint test"
  },
  "author": "Joblocal GmbH",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "core-js": "3",
    "devour-client": "2.1.0",
    "qs": "6.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/register": "7.8.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "eslint": "6",
    "eslint-config-airbnb-base": "14",
    "eslint-loader": "3",
    "eslint-plugin-import": "2.20.0",
    "git-hooks": "1.1.10",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  }
}
